# Staging Environment Configuration
# These values will be used for staging builds
NODE_ENV="development"

# Application Configuration
VITE_APP_TITLE="WebBuddy (Development)"
VITE_APP_DESCRIPTION="Development environment for WebBuddy application"

# API Configuration 
VITE_API_BASE_URL=http://0.0.0.0:8000
VITE_WS_URL=wss://0.0.0.0:8000/ws

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_API=false
VITE_ENABLE_ANALYTICS=false

# Build Configuration
VITE_BUILD_VERSION=development
# VITE_BUILD_TIMESTAMP will be set automatically during build

# External Services (Use staging/test keys)
# VITE_SENTRY_DSN=your_staging_sentry_dsn_here
# VITE_POSTHOG_KEY=your_staging_posthog_key_here

# Development Tools (Enabled in staging for debugging)
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_PREVIEW_MODE=true
