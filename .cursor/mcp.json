{
    "mcpServers": {
        "svelte5-documentation": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-svelte-docs"
            ],
            "type": "stdio"
        },
        "konva-documentation": {
            "command": "npx",
            "args": [
                "crawl-chat-mcp",
                "--id=67d221efb4b9de65095a2579",
                "--name=konva_documentation"
            ],
            "type": "stdio"
        },
        "shadcn-documentation": {
            "command": "npx",
            "args": [
                "jsrepo",
                "mcp"
            ],
            "type": "stdio"
        },
        "context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp@latest"
            ],
            "type": "stdio"
        }
    }
}
